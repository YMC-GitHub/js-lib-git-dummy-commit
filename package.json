{
  "name": "git-test-commit",
  "version": "1.0.0",
  "description": "build js lib with rollup",
  "main": "lib/git-test-commit.common.js",
  "module": "lib/git-test-commit.esm.js",
  "unpkg": "lib/git-test-commit.min.js",
  "scripts": {
    "dev": "nodemon --legacy-watch --config config/nodemon.config.json",
    "build": "node build/rollup.pro.config.js",
    "lint": "eslint --fix src test",
    "lint:no-fix": "eslint src test",
    "test": "npm run test:unit && npm run test:coverage",
    "test:unit": "rollup --config build/rollup.dev.config.js && mocha --require babel-polyfill --require babel-core/register test/*/**.js",
    "test:coverage:unit": "nyc npm run test:unit",
    "test:coverage": "nyc --reporter=html --reporter=text mocha --require babel-polyfill --require babel-core/register test/*/**.js",
    "beautify": "prettier --write \"src/*.js\" \"test/**/*.js\"",
    "release": "bash build/release.sh",
    "clean": "bash build/clean.sh",
    "changelog": "node build/change-log.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ymc-github/js-lib-git-test-commit.git"
  },
  "author": "yemiancheng <ymc.github@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ymc-github/js-lib-git-test-commit/issues"
  },
  "homepage": "https://github.com/ymc-github/js-lib-git-test-commit#readme",
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "change-log": "git+https://github.com/YMC-GitHub/js-lib-change-log.git",
    "commitlint-config-yemiancheng": "^1.0.0",
    "conventional-changelog": "^3.1.10",
    "cross-env": "^5.2.1",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-yemiancheng": "git+https://github.com/YMC-GitHub/eslint-config.git#v1.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^3.0.1",
    "lint-staged": "^9.2.0",
    "mocha": "^6.2.2",
    "mochawesome": "^4.1.0",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.0",
    "prettier": "^1.19.1",
    "rollup": "^1.17.0",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-externals": "^2.1.3",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "uglify-js": "^3.7.2"
  },
  "dependencies": {
    "shelljs": "^0.8.3"
  },
  "files": [
    "lib",
    "src"
  ]
}
